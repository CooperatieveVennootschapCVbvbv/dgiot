                    {template,"data/loaded_modules.tmpl",
                        "data/loaded_modules"},
                    {copy,"bin/nodetool","bin/nodetool"},
                    {copy,"{{output_dir}}/../../conf/schema/dgiot.schema",
                        "releases/{{rel_vsn}}/"},
                    {copy,"bin/install_upgrade_escript",
                        "bin/install_upgrade_escript"},
                    {template,
                        "{{output_dir}}/../../lib/dgiot/etc/{{vm_args_file}}",
                        "etc/vm.args"},
                    {copy,"{{output_dir}}/../../lib/dgiot/etc/certs","etc/"},
                    {copy,"{{output_dir}}/../../lib/cuttlefish/cuttlefish",
                        "bin/"}]},

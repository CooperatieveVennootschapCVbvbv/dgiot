                   [kernel,sasl,crypto,public_key,asn1,syntax_tools,ssl,jsx,
                    os_mon,inets,compiler,runtime_tools,cuttlefish,dgiot,
                    {mnesia,load},
                    {ekka,load},
                    {observer,load}]},
               {overlay,
                   [{mkdir,"etc/"},
                    {mkdir,"log/"},
                    {mkdir,"data/"},
                    {mkdir,"data/mnesia"},
                    {mkdir,"data/configs"},
                    {mkdir,"data/scripts"},
                    {template,"bin/dgiot_env","bin/dgiot_env"},

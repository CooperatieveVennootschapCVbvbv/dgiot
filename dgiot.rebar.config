                    {template,"bin/dgiot.cmd","bin/dgiot.cmd"},
                    {template,"bin/dgiot_ctl.cmd","bin/dgiot_ctl.cmd"},
                    {copy,"{{output_dir}}/../../conf/plugins","etc/"},
